<customUI onLoad="RibbonOnLoad" xmlns="http://schemas.microsoft.com/office/2009/07/customui">
  <ribbon>
    <tabs>
      <tab id="MoodleAdaptorAddIn"
          label="Exportar a Moodle">
        <group id="helpgroup"
            label="Ayuda">
          <button size="large"
              id="Logo_1_33945246"
              label="Ayuda"
              image="logoUVAeduvalab"
              visible="true"
              onAction="ShowHelp"/>
        </group>
        <group id="mso_c2.3390A5B9"
            label="Elección única">
          <button size="large"
              id="AddSingleChoiceQ_0_33945246"
              label="Nueva"
              imageMso="GoToNewRecord"
              onAction="AddSingleChoiceQ"
              supertip="Añade una nueva pregunta de solución única. [&#13;] Si su versión de Moodle no lo tiene resuelto, no olvide incluir una opción para 'No responder'"
              visible="true"/>
		  <button size="large"
              id="MarkSingleChoiceQ_0_33945246"
              label="Marcar"
              imageMso="ClickToRunUpdateOptions"
              onAction="MarkSingleChoiceQ"
              supertip="Marca el párrafo actual como pregunta de solución única. [&#13;] Si su versión de Moodle no lo tiene resuelto, no olvide incluir una opción para 'No responder'"
              visible="true"/>
          <button size="large"
              id="MarkTrueAnswer_1_33945246"
              label="Acierto/Fallo"
              imageMso="ConflictViewEnter"
              onAction="MarkTrueAnswer"
              supertip="Cambia esta línea entre la opción correcta o equivocada sucesivamente."
              visible="true"/>
          <button size="large"
              id="Notmarked_3_33945246"
              label="No respondo"
              imageMso="InkEraseMode"
              onAction="SetUnmarked"
              supertip="Marca esta línea como opción de 0 puntos que sirve para poder dejar sin responder la pregunta en caso de haberla marcado por error"
              visible="true"/>
          <button size="normal"
              id="SetAnswerWeights_2_33945246"
              label="Pesos"
              imageMso="SetPertWeights"
              onAction="SetAnswerWeights"
              supertip="Asigna pesos a las opciones de forma que se equilibren las respuestas al azar.[&#13;] Se puede cambiar manualmente"
              visible="true"/>
          <button size="normal"
              id="RemoveAnswerWeightsFromTheSelection_3_33945246"
              label="Quita pesos"
              imageMso="CancelRequest"
              onAction="RemoveAnswerWeightsFromTheSelection"
              supertip="Quita los pesos de las opciones"
              visible="true"/>
        </group>
        <group id="mso_c3.33931FA2"
            label="Elección múltiple">
          <button size="large"
              id="AddMultipleChoiceQ_4_33945246"
              label="Nueva"
              imageMso="SlideNewGallery"
              onAction="AddMultipleChoiceQ"
              supertip="Añade al final de la página una nueva pregunta de elección múltiple con varias respuestas correctas"
              visible="true"/>
		  <button size="large"
              id="MarkMultipleChoiceQ_4_33945246"
              label="Marcar"
              imageMso="ClickToRunUpdateOptions"
              onAction="MarkMultipleChoiceQ"
              supertip="Marca el párrafo actual como pregunta de elección múltiple con varias respuestas correctas"
              visible="true"/>
          <button size="large"
              id="MarkTrueAnswer_5_33945246"
              label="Acierto/Fallo"
              imageMso="ConflictViewEnter"
              onAction="MarkTrueAnswer"
              supertip="Cambia esta línea entre la opción correcta o equivocada sucesivamente."
              visible="true"/>
          <button size="normal"
              id="SetAnswerWeights_6_33945246"
              label="Pesos"
              imageMso="SetPertWeights"
              onAction="SetAnswerWeights"
              supertip="Asigna pesos a las opciones de forma que se equilibren las respuestas al azar.[&#13;] Se puede cambiar manualmente"
              visible="true"/>
          <button size="normal"
              id="RemoveAnswerWeightsFromTheSelection_7_33945246"
              label="Quita pesos"
              imageMso="CancelRequest"
              onAction="RemoveAnswerWeightsFromTheSelection"
              supertip="Quita los pesos de las opciones"
              visible="true"/>
        </group>
        <group id="mso_c1.33CB81E6"
            label="Relación">
          <button size="large"
              id="AddMatchingQ_0_33D4C4EE"
              label="Nueva"
              imageMso="ConnectorsGallery"
              supertip = "Añade una nueva pregunta de relacionar opciones. Cada línea después del enunciado se alterna entre opción izquierda y opción derecha"
              onAction="AddMatchingQ"
              visible="true"/>
        </group>
        <group id="mso_c1.33CDE828"
            label="Verdadero/Falso">
          <button size="large"
              id="AddTrueStatement_1_33D4C4EE"
              label="Hecho verdadero"
              imageMso="TagMarkComplete"
              onAction="AddTrueStatement"
              supertip="Crea una pregunta de Verdadero/Falso cuya respuesta es Verdadero"
              visible="true"/>
          <button size="large"
              id="AddFalseStatement_2_33D4C4EE"
              label="Hecho Falso"
              imageMso="Delete"
              onAction="AddFalseStatement"
              supertip="Crea una pregunta de Verdadero/Falso cuya respuesta es Falso"
              visible="true"/>
        </group>
        <group id="mso_c1.33D7BEE5"
            label="Respuesta corta">
          <button size="large"
              id="AddShortAnswerQ_0_33DA3311"
              label="Nueva"
              imageMso="MailMergeResultsPreview"
              onAction="AddShortAnswerQ"
              supertip="Crea una pregunta cuya respuesta tiene que ser un texto exacto."
              visible="true"/>
        </group>
        <group id="mso_c2.33CF3827"
            label="Numérica">
          <button size="large"
              id="AddNumericalQ_3_33D4C4EE"
              label="Nueva"
              imageMso="ConstrainNumeric"
              onAction="AddNumericalQ"
              supertip="Crea una pregunta numérica cuya respuesta es un número. [&#13;]Puede usar rangos. P.ej: 34..36"
              visible="true"/>
        </group>
        <group id="mso_c3.33CFE734"
            label="Palabra faltante">
          <button size="large"
              id="AddMissingWordQ_4_33D4C4EE"
              label="Nueva"
              imageMso="CitationMark"
              onAction="AddMissingWordQ"
              supertip="Crea una pregunta en la que alguna parte se oculta al estudiante y éste debe introducirla según el contexto. Solo se puede marcar una palabra faltante."
              visible="true"/>
          <button size="large"
              id="MarkBlankWord_5_33D4C4EE"
              label="Marcar hueco"
              imageMso="InkEraseMode"
              onAction="MarkBlankWord"
              supertip="Indica qué parte del enunciado es la parte faltante"
              visible="true"/>
        </group>
        <group id="mso_c4.33D2801F"
            label="General">
          <button size="large"
              id="AddQuestionFeedback_1_33DA3311"
              label="Realimentación"
              imageMso="CondolatoryEvent"
              onAction="AddQuestionFeedback"
              supertip="Añade una realimentación para dar información al estudiante después de responder el cuestionario. Hay que poner el cursor al final de la respuesta."
              visible="true"/>
          <control size="large"
              idMso="FileSave"
              supertip="Grabe su fichero DOC con las preguntas antes de convertirlo a GIFT para no perder el trabajo de formateo"
              visible="true"/>
          <button size="large"
              id="ExportToGIFT_6_33D4C4EE"
              label="Exportar a Moodle"
              imageMso="OutlineDemoteToBodyText"
              supertip="Este proceso transformará todo el documento en un fichero de texto plano con formato GIFT que se puede importar en Moodle"
              onAction="ExportToGIFT"
              visible="true"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>